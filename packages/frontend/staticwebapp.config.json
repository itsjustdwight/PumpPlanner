{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/api/*", "/*.css", "/*.mp4"]
    },
    "routes": [
      {
        "route": "/api/{*any}",
        "rewrite": "/api/{any}",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*.html",
        "rewrite": "/index.html"
      },
      {
        "route": "/*.css",
        "rewrite": "/index.html"
      },
      {
        "route": "/*.js",
        "rewrite": "/index.html"
      }
    ],
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
    "mimeTypes": {
      ".json": "text/json",
      ".css": "text/css"
    }
  }
  