{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/css/*"]
  },
  "routes": [
    {
      "route": "/api/{*any}",
      "rewrite": "/api/{any}",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".json": "text/json",
    ".css": "text/css"
  }
}
