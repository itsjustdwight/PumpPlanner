{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/pages/*", "/*.html", "/assets/*"]
    },
    "routes": [
      {
        "route": "/api/*",
        "allowedRoles": ["anonymous"]
      }
    ],
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }