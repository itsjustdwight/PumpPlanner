{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/api/*", "/assets/*", "/favicon.ico"]
    },
    "routes": [
      {
        "route": "/api/*",
        "allowedRoles": ["anonymous"]
      }
    ],
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }