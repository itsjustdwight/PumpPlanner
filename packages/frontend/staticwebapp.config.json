{
    "navigationFallback": {
      "rewrite": "/login.html"
    },
    "routes": [
      {
        "route": "/api/*.",
        "rewrite": "/api/",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*.html",
        "rewrite": "/login.html"
      },
      {
        "route": "/*.css",
        "rewrite": "/login.html"
      },
      {
        "route": "/*.js",
        "rewrite": "/login.html"
      }
    ],
    "mimeTypes": {
      ".json": "text/json",
      ".css": "text/css"
    }
}
  